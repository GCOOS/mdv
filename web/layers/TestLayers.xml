    <BackgroundLayers BBOX="-180,-90,180,90" server="http://ncwms.coaps.fsu.edu/geoserver/wms">
        <layer name="comm:pyrResult512" featureInfo="comm:gebco"/> 
    </BackgroundLayers>


    <!-- Optional layers -->
    <OptionalLayers BBOX="-98,18,-79,30.9"
        format="image/png"  server="http://159.203.107.216:8080/ncWMS/wms"
        ncWMS="true" max_time_range="month">
        <layer Menu="ncwms,gomexWSE"
            EN="Surface Elevation"
            name="GOMEX-PPP/surf_el_detlcl" />
    </OptionalLayers>

    <!--ncWMS Layers -->
    <MainLayers BBOX="-98,18,-79,30.9"
        format="image/png"  server="http://159.203.107.216:8080/ncWMS/wms"
        ncWMS="true" max_time_range="month">

        <layer Menu="ncwms,gomexSWV"
            EN="Velocity Streamlines"
            name="GOMEX-PPP/sea_water_velocity"
            defParticleSpeed=".9"
            overlaystreamlines="GOMEX-PPP/sea_water_velocity" />

        <layer Menu="ncwms,gomexWTMP"
            EN="Water Temperature"
            name="GOMEX-PPP/water_temp" />

        <layer Menu="ncwms,gomexSAL"
            EN="Salinity"
            name="GOMEX-PPP/salinity" />

        <layer Menu="ncwms,gomexNWV"
            EN="Northward Water Velocity"
            name="GOMEX-PPP/water_u" />

        <layer Menu="ncwms,gomexEWV"
            EN="Eastward Water Velocity"
            name="GOMEX-PPP/water_v" />
    </MainLayers>
