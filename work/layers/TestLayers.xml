<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
  <!-- Menus Begin -->
  <Menus>
    <Menu ID="observations" EN="Observations" />
    <Menu ID="models" EN="Models" />
    <Menu ID="gomex-ppp" EN="GOMEX-PPP" />
    <Menu ID="gomexSWV" EN="Seawater Velocity Streamlines" />
    <Menu ID="gomexWTMP" EN="Water Temperature" />
    <Menu ID="gomexSAL" EN="Salinity" />
    <Menu ID="gomexWSE" EN="Water Surface Elevation" />
