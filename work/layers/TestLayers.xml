  <!-- Menus End -->

  <!-- Background Layers Begin -->
  <BackgroundLayers BBOX="-180,-90,180,90" server="http://ncwms.coaps.fsu.edu/geoserver/wms">
    <layer name="comm:pyrResult512" featureInfo="comm:gebco"/>
  </BackgroundLayers>
  <!-- Background Layers End -->


  <!-- Optional Layers Begin -->
  <OptionalLayers BBOX="-98,18,-79,30.9"
    format="image/png"  server="http://159.203.107.216:8080/ncWMS/wms"
    ncWMS="true" style="vector">
    <!-- Models Begin -->
    <layer Menu="models,ncom"
      format="image/png"
      server="http://ecowatch.ncddc.noaa.gov/thredds/wms/ncom_amseas_agg/AmSeas_Apr_05_2013_to_Current_best.ncd"
      style="vector"
      name="sea_water_velocity"
      defParticleSpeed=".9"
      overlaystreamlines="surface_sea_water_velocity" />

    <layer Menu="models,nrl_std"
      format="image/png"  server="http://omgsrv1.meas.ncsu.edu:8080/thredds/wms/fmrc/sabgom/SABGOM_Forecast_Model_Run_Collection_best.ncd"
      name="NO3" />
    <!-- Models End -->

    <!-- Observations Begin -->
    <layer Menu="observations,6kmCODAR"
      format="image/png"  server="http://hfrnet.ucsd.edu/thredds/wms/HFR/USEGC/6km/hourly/RTV/HFRADAR,_US_East_and_Gulf_Coast,_6km_Resolution,_Hourly_RTV_best.ncd"
      EN="6km CODAR"
      max_time_range="month"
      style="vector"
      name="surface_sea_water_velocity"/>
    <!-- Observations End -->

  </OptionalLayers>
  <!-- Optional Layers End -->

  <!--Main Layers Begin -->
  <MainLayers BBOX="-98,18,-79,30.9"
    format="image/png"  server="http://159.203.107.216:8080/ncWMS/wms"
    ncWMS="true" max_time_range="month">

    <!-- Observations Begin -->
    <layer Menu="observations,6kmCODAR"
      format="image/png"  server="http://hfrnet.ucsd.edu/thredds/wms/HFR/USEGC/6km/hourly/RTV/HFRADAR,_US_East_and_Gulf_Coast,_6km_Resolution,_Hourly_RTV_best.ncd"
      EN="6km CODAR"
      style="vector"
      name="surface_sea_water_velocity" />
    <!-- Observations End -->

    <!-- Models Begin -->
    <!-- NRL MEAN Begin -->
    <layer Menu="models,nrl_mean,nrl_water_temp"
      format="image/png"
      server="http://gcoos4.tamu.edu:8080/thredds/wms/nrl_mean/out/2016_MEAN_best.ncd"
      EN="NRL Mean Water Temp"
      name="water_temp" />

    <layer Menu="models,nrl_mean,nrl_water_u"
      format="image/png"
      server="http://gcoos4.tamu.edu:8080/thredds/wms/nrl_mean/out/2016_MEAN_best.ncd"
      EN="NRL Mean Water U"
      name="water_u" />

    <layer Menu="models,nrl_mean,nrl_water_v"
      format="image/png"
      server="http://gcoos4.tamu.edu:8080/thredds/wms/nrl_mean/out/2016_MEAN_best.ncd"
      EN="NRL Mean Water V"
      name="water_v" />

   <layer Menu="models,nrl_mean,nrl_surf_el_detlcl"
      format="image/png"
      server="http://gcoos4.tamu.edu:8080/thredds/wms/nrl_mean/out/2016_MEAN_best.ncd"
      EN="NRL Mean Surface Elevation"
      name="surf_el_detlcl" />
   <!-- NRL MEAN END -->

   <!-- NCOM BEGIN -->
   <layer Menu="models,ncom"
      format="image/png"
      server="http://ecowatch.ncddc.noaa.gov/thredds/wms/ncom_amseas_agg/AmSeas_Apr_05_2013_to_Current_best.ncd"
      EN="NCOM Velocity"
      style="vector"
      name="sea_water_velocity" />
   <!-- NCOM END -->

    <!-- Models End -->

  </MainLayers>
